<IfModule mod_headers.c>
  Header set Access-Control-Allow-Origin "https://lutramontana.sk" Header set
  Access-Control-Allow-Methods "GET,HEAD,PUT,PATCH,POST,DELETE,OPTIONS" Header
  set Access-Control-Allow-Headers "Origin, X-Requested-With, Content-Type,
  Accept, Authorization" Header set Access-Control-Allow-Credentials "true"
</IfModule>
